@model IEnumerable<AssetListDTO>

<div class="container-fluid px-4">
    <div class="card mb-4">
        <div class="card-header">
            <ul class="header-ul mt-2">
                <li class="header-li">Kullanımda Olanlar <span class="tm-top-number" style=" font-size: 14px">305</span></li>
            </ul>
        </div>
        <div class="card-body">
            <table id="datatablesAllAsset">
                <thead>
                    <tr>
                        <th></th>
                        <th>Kayıt Numarası</th>
                        <th>Barkod</th>
                        <th>Ürün Tipi</th>
                        <th>Ürün Güncel Fiyatı</th>
                        <th>Markası</th>
                        <th>Modeli</th>
                    </tr>
                </thead>
                <tbody>
                        @foreach (var item in Model)
                        {
                            <tr>
                                <th></th>
                                <td>@item.RegistrationNumber</td>
                                <td>@item.Barcode</td>
                                <td>@item.AssetType</td>
                                <td>@item.CurrentPrice</td>
                                <td>@item.AssetBrand</td>
                                <td>@item.AssetModel</td>
                            </tr>
                        }

                </tbody>
            </table>
        </div>
    </div>
</div>